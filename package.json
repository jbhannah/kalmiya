{
  "name": "app",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.2",
    "autoprefixer": "^10.4.17",
    "esbuild": "^0.20.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-nesting": "^12.0.2"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds",
